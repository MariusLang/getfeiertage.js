{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "function-paren-newline": "off",
        "linebreak-style": [
            "error",
            "unix"
        ]
    }
}